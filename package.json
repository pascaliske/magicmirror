{
  "name": "magicmirror",
  "description": "",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "install": "ansible-galaxy install -r requirements.yml && cd server && go mod tidy -compat=1.17",
    "postinstall": "ngcc --tsconfig tsconfig.json",
    "start": "ng serve",
    "start:server": "cd server && gow run -ldflags=\"$LDFLAGS\" main.go --config ../config.yml",
    "build": "ng build",
    "lint": "ng lint",
    "format": "prettier --write \"**/*.{ts,scss,md,html,json,yml}\"",
    "ssh": "ssh -t -i \"$ANSIBLE_PRIVATE_KEY_FILE\" \"$SSH_USER@$SSH_HOST\"",
    "playbook:provision": "ansible-playbook --diff --ask-vault-pass --extra-vars @ansible/vars/vault.yml ansible/playbooks/provision.yml",
    "playbook:update": "ansible-playbook --diff --ask-vault-pass --extra-vars @ansible/vars/vault.yml ansible/playbooks/update.yml",
    "vault:encrypt": "ansible-vault encrypt ansible/vars/vault.yml",
    "vault:decrypt": "ansible-vault decrypt ansible/vars/vault.yml",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog"
  },
  "private": true,
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "@angular/animations": "^14.1.3",
    "@angular/common": "^14.1.3",
    "@angular/compiler": "^14.1.3",
    "@angular/core": "^14.1.3",
    "@angular/forms": "^14.1.3",
    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "@angular/service-worker": "^14.1.3",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngrx/effects": "^14.2.0",
    "@ngrx/store": "^14.2.0",
    "@ngrx/store-devtools": "^14.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@pascaliske/ngx-sentry": "^4.1.0",
    "@sentry/browser": "^7.13.0",
    "date-fns": "^2.29.3",
    "modern-normalize": "^1.1.0",
    "modularscale-sass": "^3.0.10",
    "ngx-progressbar": "^9.0.0",
    "rxjs": "~7.5.6",
    "ts-cacheable": "^1.0.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.1",
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "14.1.1",
    "@angular-eslint/eslint-plugin": "14.0.4",
    "@angular-eslint/eslint-plugin-template": "14.0.4",
    "@angular-eslint/schematics": "14.0.4",
    "@angular-eslint/template-parser": "14.0.4",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.1.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@pascaliske/eslint-config": "^2.0.6",
    "@pascaliske/prettier-config": "^1.2.0",
    "@rstacruz/bump-cli": "^2.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-changelog": "^2.0.27",
    "typescript": "~4.6.4"
  }
}
